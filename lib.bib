}

@article{CollinearityIsnDisease,
  title = {Collinearity Isn't a Disease That Needs Curing},
  pages = {15},
  abstract = {Once they have learnt about the effects of collinearity on the output of multiple regression models, researchers may unduly worry about these and resort to dubious modelling techniques to mitigate them. This article argues that, to the extent that problems occur in the presence of collinearity, they are not caused by it but rather by common mental shortcuts that researchers take when interpreting statistical model that can also lead them astray in the absence of collinearity. Moreover, I illustrate that common strategies for dealing with collinearity only sidestep the perceived problem by biasing 5 the parameters, reformulating the model in such a way that it maps onto different research questions, or both. I conclude that collinearity in itself is not a problem and that researchers should be aware of what their approaches for addressing it actually achieve.},
  file = {/home/aleksandar/Dropbox/Zotero/undefined/Collinearity isn't a disease that needs curing.pdf},
  language = {en}
}

@article{colomboLearningHighdimensionalDirected2012,

@article{briceno-rosasImprovingCentralMonitoring2020,
  title = {Improving {{Central Monitoring}} of {{Fieldwork}} in {{Cross}}-National {{Surveys}}: {{The Case}} of the {{Fieldwork Management System}} in the {{European Social Survey}}},

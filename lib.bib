@article{boeingVisualAnalysisNonlinear2016,
  title = {Visual {{Analysis}} of {{Nonlinear Dynamical Systems}}: {{Chaos}}, {{Fractals}}, {{Self}}-{{Similarity}} and the {{Limits}} of {{Prediction}}},

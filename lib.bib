  author = {Liu, Wei and Yan, Zhengxin and Zhou, Gaoliang},
  doi = {10.1515/phys-2019-0001},
  abstract = {{$<$}p{$>$}This article offers a detailed analysis of the Ising model in 2D small-world networks with competing Glauber and Kawasaki dynamics. The non-equilibrium stationary state phase transitions are obtained in these networks. The phase transitions are discussed, and the phase diagrams are obtained via Monte Carlo simulations and finite-size analyzing. We find that as the addition of links increases the phase transition temperature increases and the transition competing probability of tricritical point decreases. For the competition of the two dynamics, ferromagnetic to anti-ferromagnetic phase transitions and the critical endpoints are found in the small-world networks.{$<$}/p{$>$}},
  journal = {Open Physics}
}


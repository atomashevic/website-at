}

@article{boesePatternsDemocracySpace2019,
  title = {Patterns of {{Democracy}} over {{Space}} and {{Time}}},
  author = {Boese, Vanessa A and Gates, Scott and Knutsen, Carl Henrik},
  year = {2019},
  pages = {27},
  abstract = {Dichotomous and unidimensional measures of democracy fail to account for the multidimensional nature of political systems. This article disaggregates the concept of democracy and proposes a novel multidimensional conceptualization to account for the variation in institutional configurations. Three dimensions are featured: participation, electoral contestation, and constraints on the executive. The three dimensions constitute a cube covering all regime types using V-Dem data from 1789 to 2017. Eckstein (1973) theorized that we should expect to see systematic patterns of institutional consistency, whereby sets of similar institutions occur together. Our multidimensional measure allows for a more comprehensive test of the Eckstein hypothesis. We find strong evidence to support Eckstein (1973). First, across the globe over 200 years, there are configurations of political institutions that never occur. In other words, incompatible institutional pairs do not exist or are extremely short lived. Second, we are able to differentiate political systems that appear similar along a unidimensional scale (e.g Hong Kong and Equador) but are in reality quite distinct. Third, we are able to trace historical patterns of democratization and autocratization (especially in the early stages of the development of democracy) using our cube of democracy patterns.},
  language = {en}
}


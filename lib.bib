@article{kozitsinOpinionConvergenceKrasnoshchekov,
  title = {Opinion Convergence in the {{Krasnoshchekov}} Model},
  author = {Kozitsin, Ivan Vladimirovich and Belolipetskii, Alexander Alexeyevich},
  doi = {10.1080/0022250X.2018.1531398},
  abstract = {In this paper, a rigorous mathematical analysis of the Krasnoshchekov model is presented. We have shown that in case a community does not contain any group of people having zero resistance to interpersonal influence, which are moreover isolated from the pressure of the rest of community, the Krasnoshchekov opinion readjustment procedure can be reduced to the Friedkin\textendash Johnsen dynamics. In turn, if one repeats the Krasnoshchekov opinion updating rule, the corresponding dynamics forces individuals' opinions to converge eventually to some terminal opinions, which are a consensus under the same conditions as in the French\textendash Harary\textendash DeGroot dynamics. Otherwise, the Krasnoshchekov dynamics exhibits patterns, which are much closer to the behavior of electrons in the superconductivity state.},
  journal = {Journal of Mathematical Sociology}
}


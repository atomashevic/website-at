@article{duarteDiscreteStatisticalModels,
  title = {Discrete {{Statistical Models}} with {{Rational Maximum Likelihood Estimator}}. ({{arXiv}}:1903.06110v1 [Math.{{ST}}])},
  author = {href="http://arxiv.org/find/math/1/au:+Duarte\_E/0/1/0/all/0/1"{$>$}Eliana Duarte{$<$}/a{$>$} and href="http://arxiv.org/find/math/1/au:+Marigliano\_O/0/1/0/all/0/1"{$>$}Orlando Marigliano{$<$}/a{$>$} and href="http://arxiv.org/find/math/1/au:+Sturmfels\_B/0/1/0/all/0/1"{$>$}Bernd Sturmfels{$<$}/a{$>$}},
  doi = {arXiv:1903.06110v1},
  abstract = {{$<$}p{$>$}A discrete statistical model is a subset of a probability simplex. Its maximum likelihood estimator (MLE) is a retraction from that simplex onto the model. We characterize all models for which this retraction is a rational function. This is a contribution via real algebraic geometry which rests on results due to Huh and Kapranov on Horn uniformization. We present an algorithm for constructing models with rational MLE, and we demonstrate it on a range of instances. Our focus lies on models familiar to statisticians, like Bayesian networks, decomposable graphical models, and staged trees. {$<$}/p{$>$}},
  journal = {arXiv Statistics Theory}
}


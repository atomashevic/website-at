@article{desaiTheoryElectoralCompetition,
  title = {A {{Theory}} of {{Electoral Competition}} in {{Developing Countries}}},
  author = {Desai, Zuheir},
  pages = {61},
  abstract = {Why does the traditional left-right (L-R) dimension of politics not structure political competition in developing countries, while other political issues, such as ethnicity appear more relevant? Does identity-based mobilization by political parties persist or subside with development? I propose a theory of electoral competition that highlights why the L-R dimension can appear unimportant in developing countries, even if it is salient to voters. Competitive pressures force parties to cluster together on the L-R dimension in developing democracies, even if they are ideologically opposed. Economic development creates a larger affluent class, which leads parties to diverge more along the L-R dimension, thereby increasing its political salience. The right party mobilizes more along ethnic lines when parties compete on both economic policies and identity. In contrast, the relationship between development and the political salience of ethnicity depends on the correlation between ethnic preferences and economic class. When ethnic preferences are independent of economic class, development increases ethnic mobilization.},
  language = {en}
}


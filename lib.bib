  archivePrefix = {arXiv},
  eprint = {2006.01764},
  eprinttype = {arxiv},
  file = {/home/aleksandar/Documents/Zotero/Battiston et al/Battiston et al_2020_Networks beyond pairwise interactions.pdf},

@article{michalskiEffectiveInfluenceSpreading2020,
  title = {Effective {{Influence Spreading}} in {{Temporal Networks With Sequential Seeding}}},
  author = {Michalski, Rados{\l}aw and Jankowski, Jaros{\l}aw and Br{\'o}dka, Piotr},
  year = {2020},

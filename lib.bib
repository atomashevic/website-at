@misc{tianUnifiedBayesianModel2020,
  title = {A {{Unified Bayesian Model}} for {{Generalized Community Detection}} in {{Attribute Networks}}},
  author = {Tian, Qiang and Wang, Wenjun and Xie, Yingjie and Wu, Huaming and Jiao, Pengfei and Pan, Lin},
  year = {2020},

@article{fraimanStochasticRecursionsDirected2020,
  title = {Stochastic Recursions on Directed Random Graphs},
  author = {Fraiman, Nicolas and Lin, Tzu-Chi and {Olvera-Cravioto}, Mariana},
  year = {2020},
  month = oct,
  abstract = {For a directed graph \$G(V\_n, E\_n)\$ on the vertices \$V\_n = \textbackslash\{1,2, \textbackslash dots, n\textbackslash\}\$, we study the distribution of a Markov chain \$\textbackslash\{ \{\textbackslash bf R\}\^\{(k)\}: k \textbackslash geq 0\textbackslash\}\$ on \$\textbackslash mathbb\{R\}\^n\$ such that the \$i\$th component of \$\{\textbackslash bf R\}\^\{(k)\}\$, denoted \$R\_i\^\{(k)\}\$, corresponds to the value of the process on vertex \$i\$ at time \$k\$. We focus on processes \$\textbackslash\{ \{\textbackslash bf R\}\^\{(k)\}: k \textbackslash geq 0\textbackslash\}\$ where the value of \$R\_i\^\{(k+1)\}\$ depends only on the values \$\textbackslash\{ R\_j\^\{(k)\}: j \textbackslash to i\textbackslash\}\$ of its inbound neighbors, and possibly on vertex attributes. We then show that, provided \$G(V\_n, E\_n)\$ converges in the local weak sense to a marked Galton-Watson process, the dynamics of the process for a uniformly chosen vertex in \$V\_n\$ can be coupled, for any fixed \$k\$, to a process \$\textbackslash\{ \textbackslash mathcal\{R\}\_\textbackslash emptyset\^\{(r)\}: 0 \textbackslash leq r \textbackslash leq k\textbackslash\}\$ constructed on the limiting marked Galton-Watson process. Moreover, we derive sufficient conditions under which \$\textbackslash mathcal\{R\}\^\{(k)\}\_\textbackslash emptyset\$ converges, as \$k \textbackslash to \textbackslash infty\$, to a random variable \$\textbackslash mathcal\{R\}\^*\$ that can be characterized in terms of the so-called special endogenous solution to a branching distributional fixed-point equation. Our framework can be applied also to processes \$\textbackslash\{ \{\textbackslash bf R\}\^\{(k)\}: k \textbackslash geq 0\textbackslash\}\$ whose only source of randomness comes from the realization of the graph \$G(V\_n, E\_n)\$.},
  archivePrefix = {arXiv},
  eprint = {2010.09596},
  eprinttype = {arxiv},
  file = {/home/aleksandar/Documents/Zotero/Fraiman et al/Fraiman et al_2020_Stochastic recursions on directed random graphs.pdf;/home/aleksandar/Zotero/storage/NBQZZBSU/2010.html},
